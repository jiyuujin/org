{
  "extends": ["plugin:@nrwl/nx/react-typescript", "../../.eslintrc.json", "next"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ],
  "env": {
    "jest": true
  },
  "rules": {
    "@next/next/no-img-element": 0,
    "@typescript-eslint/no-explicit-any": 0,
    "@typescript-eslint/ban-ts-comment": 0,
    "react/display-name": 0
  }
}
