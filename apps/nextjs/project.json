{
  "root": "apps/nextjs",
  "sourceRoot": "apps/nextjs",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/nextjs",
        "outputPath": "dist/apps/nextjs"
      },
      "configurations": {
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "options": {
        "buildTarget": "nextjs:build",
        "dev": true
      },
      "configurations": {
        "production": {
          "buildTarget": "nextjs:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "nextjs:build:production"
      }
    },
    "tsm": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "tsm src",
        "cwd": "apps/nextjs",
        "parallel": false
      }
    },
    "lint": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .gitignore",
          "eslint ./src --ext .tsx,.ts,.jsx,.js --fix",
          "stylelint --config .stylelintrc.yml 'src/styles/**/*.scss' --fix"
        ],
        "cwd": "apps/nextjs",
        "parallel": false
      }
    }
  },
  "tags": []
}
